<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.MainViewController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label text="Main">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets left="300.0" />
               </VBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#customersBtnPressed" prefHeight="27.0" prefWidth="95.0" text="Customers">
               <VBox.margin>
                  <Insets left="275.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#apptsBtnPressed" text="Appointments">
               <VBox.margin>
                  <Insets left="275.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#reportsBtnPressed" prefHeight="27.0" prefWidth="95.0" text="Reports">
               <VBox.margin>
                  <Insets left="275.0" top="20.0" />
               </VBox.margin>
            </Button>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="282.0" prefWidth="600.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#exitBtnPressed" prefHeight="27.0" prefWidth="50.0" text="Exit">
                     <HBox.margin>
                        <Insets bottom="10.0" right="10.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
